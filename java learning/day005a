package myArrays;
import java.util.Arrays;
public class sumOfElements {
	public static int sum(int[] a) {
		int sum=0;
		for(int i=0;i<a.length;i++) {
			sum+=a[i];
		}
		// sum of elements return sum;
		return sum/a.length;
	}
	public static void main(String[] args) {
		int[] a= {1,2,3,4,5};
		int b=sum(a);
		System.out.println(b);
	}

}