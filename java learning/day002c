package programs;
public class Sweth{
public static int isNumber(int n) {
		for(int i=0;i<=n;i++) {
			System.out.println(+i);  
		}
		return n;
	}
	//write a code to print even numbers from 1 to n
	public static int isNumberOfEven(int n) {
		for(int i=0;i<=n;i++) {
			if(i%2==0) {
				System.out.println(+ i);
			}
		}
		return n;
	}
}