package Mini;
import java.util.Scanner;
public class Nestedif {
	public static void main(String[] args) {
		
		boolean isStudent=true;
		boolean isSenior=false;
		double price=9.99;
		
		if(isStudent) {
			if(isSenior) {
				System.out.println("You got a 20% discount");
				price*=0.8;
			}
			else {
				System.out.println("You got a student discount 10%:");
				price*=0.9;
			}
		}
		else {
			if(isSenior) {
				System.out.println("You got a senior discount of 20%");
				price*=0.8;
			}
			else {
				price*=1;
			}
		}
			
		}
	}

