class Student:
    def __init__(self,marks1,marks2):
        self.m1=marks1
        self.m2=marks2

    def __add__(self,other):
        total1=self.m1+other.m1
        total2=self.m2+other.m2
        return Student(total1,total2)

    def __str__(self):
        return f"Marks1={self.m1},Marks2={self.m2}"

s1=Student(40,20)
s2=Student(50,50)
s3=s1+s2
print(s3)