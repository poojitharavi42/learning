<body>
    <script>
        function isAttendance(time){
            if(time<=9.00){
                console.log('On time');
            }else if(time>9.00 && time<9.30){
                console.log('Late.');
            }else{
                console.log('Half day.');
            }
        }
        isAttendance(10.00);
    </script>
</body>
